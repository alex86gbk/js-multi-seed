{
  "compilerOptions": {
    "baseUrl": "./src",
    "moduleResolution": "node",
    //"allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "jsx": "preserve",
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "target": "es6",
    "lib": [
      "dom",
      "es2017"
    ],
    "skipLibCheck": true,
  },
  "exclude": [
    "assets",
  ]
}
