{
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": "./",
    "paths": {
      "src/*": [
        "./src/*"
      ]
    },
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "jsx": "preserve",
    "noUnusedLocals": true,
    "target": "es6",
    "lib": [
      "dom",
      "es2017"
    ],
    "skipLibCheck": true,
    "outDir": "/"
  },
  "exclude": [
    ".usr",
    ".var",
    ".projectrc.js",
    "assets",
    "mock",
    "public",
    "themes",
    "node_modules",
  ]
}
