    "dev": "npm run devWebpack && cross-env API=dev webpack-dev-server",
    "build": "cross-env NODE_ENV=production webpack --config webpack.production.config.js && npm run version",
    "analyzer": "cross-env NODE_ENV=production webpack --config webpack.production.config.js --profile --json > stats.json && webpack-bundle-analyzer stats.json",
    "version": "gulp version",
    "compass": "gulp compass",
    "gemCompass": "gem sources -a https://ruby.taobao.org/ --remove https://rubygems.org/ && gem install compass -v 1.0.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alex86gbk/js-multi-seed.git"
  },
